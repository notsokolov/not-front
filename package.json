{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint ./src/pages/* ./src/components/* ./src/api/* ./src/data/* ./src/graphql/* ./src/lib/* ./src/types/* -c .eslintrc.json --fix --ext ts,tsx",
    "firebase": "cd src/firebase && firebase deploy --only functions && cd ../",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch 'src/graphql/**/*.ts'",
    "format": "prettier --write **/*.ts **/*.tsx",
    "postinstall": "yarn generate"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.8",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@tippy.js/react": "^3.1.1",
    "@zeit/next-source-maps": "^0.0.3",
    "cookie": "^0.4.1",
    "graphql": "^15.5.0",
    "graphql-tools-fork": "^9.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.8",
    "postmark": "^2.7.3",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-markdown": "^5.0.3",
    "reflexbox": "^4.0.6",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.0.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-stringify": "^9.0.1",
    "serve-favicon": "^2.5.0",
    "string-replace-to-array": "^1.0.3",
    "styled-components": "^5.2.1",
    "terser": "^5.6.0",
    "theme-ui": "^0.3.5",
    "throttle-debounce": "^3.0.1",
    "timeago-react": "3.0.2",
    "timeago.js": "^4.0.2",
    "unified": "^9.2.1",
    "unified-stream": "^1.0.6"
  },
  "devDependencies": {
    "@ardatan/graphql-tools": "^4.1.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@testing-library/cypress": "^7.0.1",
    "@types/chai": "^4.2.14",
    "@types/jest": "^26.0.15",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.7",
    "@types/react": "^16.14.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "lint-staged": "^10.5.1",
    "postcss": "^8.1.9",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "prettier-eslint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.1",
