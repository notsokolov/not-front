    "next": "^10.0.8",
    "postmark": "^2.7.3",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-markdown": "^5.0.3",
    
    "rehype-parse": "^7.0.1",
